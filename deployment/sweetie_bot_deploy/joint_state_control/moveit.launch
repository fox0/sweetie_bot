<!-- MODULE: run move_group in current LOCAL context -->
<launch>
	<arg name="machine" default="localhost" />  <!-- macine to run nodes -->

	<!-- Load the URDF, SRDF and other .yaml configuration files on the param server -->
	<include file="$(find sweetie_bot_moveit_config_proto2)/launch/planning_context.launch">
		<arg name="load_robot_description" value="true"/>
	</include>
	
	<!-- Run the main MoveIt executable  -->
	<group>
		<remap from="/tf" to="tf"/>
		<remap from="/tf_static" to="tf_static"/>
		<include file="$(find sweetie_bot_moveit_config_proto2)/launch/move_group.launch">
			<!-- <arg name="machine" value="$(arg machine)"/> -->
			<arg name="info" value="true"/>
			<arg name="jiggle_fraction" default="0.2" />
		</include>
	</group>
</launch>
